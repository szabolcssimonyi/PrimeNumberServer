{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://primenumber.log:9200"
  },
  "CacheName": "primenumber.cache",
  "Range": {
    "min": 2,
    "max": 100000
  },
  "AllowedHosts": "*"
}
